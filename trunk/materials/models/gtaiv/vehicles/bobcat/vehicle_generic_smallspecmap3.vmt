"VertexLitGeneric"
{
	"$baseTexture" "models\gtaiv\vehicles\diamond\bentgray"
	"$bumpmap" "models\gtaiv\vehicles\oracle\veh_norm"
	"$envmap" "env_cubemap"
	"$envmaptint" "[.4 .4 .4]"
	"$phong" "1"
	"$phongexponent" "20"
	"$phongboost" ".3"	
	"$phongfresnelranges"	"[.3 1 8]"
