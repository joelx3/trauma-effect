"LightmappedGeneric"
{
   "$basetexture" "city_8/monitor_animated"
   "$envmap" "env_cubemap"	
   "$bumpmap" "city_8/hex_normal"
   "$normalmapalphaenvmapmask" 1
   	"$surfaceprop" "glass"
	"$additive" "1"
   "Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "0.5"
      }
   }
}