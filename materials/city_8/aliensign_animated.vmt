"LightmappedGeneric"
{
   "$basetexture" "city_8/aliensign_animated"
   "$envmap" "env_cubemap"	
   "$bumpmap" "city_8/hex_normal"
   "$normalmapalphaenvmapmask" 1
	"$additive" "1"
	"$selfillum" "1"
		"$surfaceprop" "glass"
   "Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "10"
      }
   }
}